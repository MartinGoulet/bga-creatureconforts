{OVERALL_GAME_HEADER}

<div id="table">
    <div id="table-score"></div>
    <div id="tables-and-center">
        <div id="current-player-table"></div>
        <div id="table-center-wrapper">
            <div id="table-improvements"></div>
            <div id="glade"></div>
            <div id="board">
                <div id="river-dial" data-position="5"></div>
                <div id="table-comforts" class="white-border"></div>
                <div id="table-travelers"></div>
                <div id="table-valleys"></div>
                <div id="worker-locations"></div>
                <div id="dice-locations"></div>
                <div id="hill-dice"></div>
                <div id="reserved-zones"></div>
                <div id="american-beaver-zones"></div>
                <div id="black-bear-zones"></div>
                <div id="leopard-frog-zones"></div>

                <div id="decks">
                    <div>
                        <div id="deck-improvements"></div>
                        <div id="discard-improvements"></div>
                    </div>
                    <div>
                        <div id="deck-comforts"></div>
                        <div id="discard-comforts"></div>
                    </div>
                </div>
            </div>
            <div id="discard-comforts-wrapper" class="whiteblock">
                <div class="title">Discard</div>
                <div id="discard-comforts-line"></div>
            </div>
        </div>
        <div id="tables"></div>
    </div>
</div>

{OVERALL_GAME_FOOTER}
